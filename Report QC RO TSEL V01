eNodeBID = df.loc[0,'OSS LTE eNodeB id']
PONumber = df.loc[0,'PMO']
SOWco = df.loc[0,'LAYER']
alamat = df.loc[0,'Address']
cluster = df.loc[0,'CLUSTER NEW']
#data sheet GE
SiteName = eNodeBName
Tipetower = df.loc[0,'Site Type']
TowerHeigth = df.loc[0,'Height']
Long = df.loc[0,'Longitude']
Lat = df.loc[0,'Latitude']
#data sheet data audit
SectorID = eNodeBName
AntennaType = df.loc[0,'Antenna type']
Height = TowerHeigth
