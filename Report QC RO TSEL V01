        if i in os.listdir(pic_path) and i == ussd[2]:
            left=sh_ping.Cells(1,11).Left #(1,required_coloumn)
            top=sh_ping.Cells(56,1).Top #(required_row,1)
            sh_ping.Shapes.AddPicture(f'{pic_path}/{ussd[2]}',LinkToFile=False, SaveWithDocument=True, Left=left, Top=top, Width=uwidth, Height=uheight)
    except:
        pass
print('sheet PING done')
wb.save
wb.Close()
#Excel.Quit()
